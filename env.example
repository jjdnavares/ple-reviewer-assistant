# Example environment variables for MedReview
# Copy to .env and fill in with your actual secrets

# --- Frontend ---
NEXT_PUBLIC_API_URL=http://localhost:3001
NEXT_PUBLIC_APP_NAME=MedReview

# --- Backend ---
DATABASE_URL=postgresql://user:password@localhost:5432/medreview
JWT_SECRET=your_jwt_secret
OPENAI_API_KEY=your_openai_key
REDIS_URL=redis://localhost:6379
